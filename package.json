{
  "name": "gremlins-ts",
  "version": "0.2.0-beta.1",
  "description": "A monkey testing library written in JavaScript, for Node.js and the browser. Use it to check the robustness of web applications by unleashing a horde of undisciplined gremlins.",
  "main": "dist/gremlins.min.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "format": "prettier --write {src/**/*.ts,webpack.config.js}",
    "types": "tsc --noEmit",
    "build": "webpack --mode=production"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/marmelab/gremlins.js.git"
  },
  "keywords": [
    "monkey",
    "test",
    "testing",
    "stress",
    "gremlin"
  ],
  "author": "Francois Zaninotto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/obartra/gremlins.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.2",
    "@babel/preset-typescript": "^7.3.3",
    "eslint": "^6.0.1",
    "prettier": "^1.18.2",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "@types/chance": "^1.0.5",
    "@types/node": "^12.6.1",
    "chance": "^1.0.18"
  }
}
